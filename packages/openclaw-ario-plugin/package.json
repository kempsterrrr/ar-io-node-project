{
  "name": "@ar-io/openclaw-ario-plugin",
  "version": "0.1.0",
  "description": "OpenClaw plugin providing tools for interacting with AR.IO gateways and Arweave",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
