    "@types/node": "^22.10.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "arweave",
    "ar-io",
    "gateway"
  ]
